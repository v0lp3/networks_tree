source /etc/network/interfaces.d/*

allow-hotplug enp0s0
iface enp0s0 inet static
address 10.0.128.10/29

allow-hotplug enp0s1
iface enp0s1 inet static
address 10.0.129.107/24

allow-hotplug enp0s2
iface enp0s2 inet static
address 10.0.131.64/23

up /bin/ip route add 10.0.132.0/22 via 10.0.128.11
up /bin/ip route add 10.0.128.64/26 via 10.0.128.11
