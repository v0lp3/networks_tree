source /etc/network/interfaces.d/*

allow-hotplug enp0s0
iface enp0s0 inet static
address 10.0.128.1/30

up /bin/ip route add 10.0.132.0/22 via 10.0.128.0
up /bin/ip route add 10.0.128.64/26 via 10.0.128.0
up /bin/ip route add 10.0.129.0/24 via 10.0.128.0
up /bin/ip route add 10.0.130.0/23 via 10.0.128.0
up /bin/ip route add 10.0.128.8/29 via 10.0.128.0
